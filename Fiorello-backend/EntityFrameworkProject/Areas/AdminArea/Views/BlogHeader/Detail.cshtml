@model BlogHeader
@{
    ViewData["Title"] = "Detail";
}


<div class="container my-5">
    <div class="card">
        <div class="card-header">
            Product detail
        </div>
        <div class="card-body">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>
                            Header 
                        </th>
                       
                        <th>
                           Header Description
                        </th>
                    </tr>
                </thead>
                <tbody>

                    <tr>

                        
                        <td>
                            @Html.Raw(Model.Header)
                        </td>
                        <td>
                            @Html.Raw(Model.HeaderDesc)
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <a class="btn btn-primary my-4" asp-controller="BlogHeader" asp-action="Index">Back</a>
</div>

